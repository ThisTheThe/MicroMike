.mod-right-split .workspace-tab-header:hover
{
	background-color: var(--level1_a);
}
.workspace-tab-header:hover
{
	background-color: var(--level1_a);
}
.workspace-tab-header.is-active
{
	background-color: var(--level1_b);
}
.workspace-tab-header.is-active:hover,
.workspace-tab-header.mod-active.is-active:hover
{
	background-color: var(--level1_b);
}
.mod-left-split .workspace-tab-header.has-active-menu, 
.mod-right-split .workspace-tab-header.has-active-menu, 
.mod-left-split .workspace-tab-header.is-active, 
.mod-right-split .workspace-tab-header.is-active
{
	background-color: var(--level1_b);
}

@media (hover: hover) {
  .mod-left-split .workspace-tab-header.has-active-menu:hover,
  .mod-right-split .workspace-tab-header.has-active-menu:hover,
  .mod-left-split .workspace-tab-header.is-active:hover,
  .mod-right-split .workspace-tab-header.is-active:hover {
    background-color: var(--level1_b);
  }
}

.workspace-tab-header-inner
{
	height:var(--tabHeightSidebar);
}
.workspace-tab-header:not(.is-active):hover .workspace-tab-header-inner
{
	background-color:transparent;
}.sidebar-toggle-button.mod-left
{
	padding-bottom:0px;
}
.sidebar-toggle-button.mod-left .clickable-icon
{
	opacity:1;
	width:40px;
	height:40px;
	background-color:var(--level1);
	
}

.sidebar-toggle-button.mod-left .clickable-icon:hover
{
	background-color:var(--level1_a);
}
.is-collapsed .sidebar-toggle-button.mod-left .clickable-icon
{
	background-color:var(--level1_b);
}
.workspace-ribbon.side-dock-ribbon.mod-left::before
{
	display:none;
}
.is-collapsed .sidebar-toggle-button.mod-left .clickable-icon {
    background-color: var(--level2);
}
.is-collapsed .sidebar-toggle-button.mod-left .clickable-icon:hover {
    background-color: var(--level2_a);
}.sidebar-toggle-button.mod-right
{
	justify-items:center;
	padding-bottom:0px;
	position:fixed;
	top:0px;
	right:120px;
	height:40px;
	width:40px;
	z-index:10;
}
.sidebar-toggle-button.mod-right .clickable-icon
{
	width:100%;
	height:100%;
	background-color:var(--level1);
	opacity:1;
	bottom:-1px;
}
.sidebar-toggle-button.mod-right .clickable-icon:hover
{
	background-color:var(--level1_a);
}
.mod-top-right-space .sidebar-toggle-button.mod-right .clickable-icon
{
	background-color:var(--level2);
}
.mod-top-right-space .sidebar-toggle-button.mod-right .clickable-icon:hover
{
	background-color:var(--level2_a);
}.status-bar
{
	border-top:1px solid var(--level2);
}
.status-bar-item
{
	border-left:1px solid var(--level2);
	padding-left:5px;
	padding-right:5px;
}/* Tags in editor / preview */

a.tag, .cm-hashtag
{
	padding:0px !important;
	border-radius:0px;
	font-size: var(--font-text-size);
	background-color: transparent;
}
/* Tag Pane */
.workspace-leaf-content[data-type=tag]
{
	padding:0px;
	margin:0px;
}
.tag-pane-tag-count
{
	background-color:var(--background);
	color:var(--fontCol);
}
.tag-container
{
	background-color: var(--backgroundDeep);
	margin-left:4px;
	padding-top:3px;
}
.tag-pane-tag
{
	padding:0;
	border-style:none none solid none;
	border-color: var(--intermediary);
	border-width: 1px;
}
.tag-container.node-insert-event
{
	padding:0px;
	margin-right:4px;
	overflow: hidden;

}
.tag-container .tree-item-icon.collapse-icon
{
	left:10px;
}.titlebar-button-container.mod-right
{
	height:40px;
	
}
.titlebar-button-container .titlebar-button
{
	width:40px;
	box-shadow: var(--bevelStyleButton);
}
.titlebar-button-container .titlebar-button:hover
{
	background-color:var(--level2);
}
.titlebar-button-container .titlebar-button svg
{
	width:10px;
}
.mod-linux .titlebar-button, 
.mod-windows .titlebar-button
{
	padding-left:15px;
	padding-right:15px;
}
.is-hidden-frameless:not(.is-fullscreen) .titlebar-button-container.mod-right,
.is-hidden-frameless:not(.is-fullscreen).is-focused .titlebar-button-container.mod-right
{
	background-color:var(--bottom);
}

/* This is what allows us to click the window buttons, I have learned the hard way */
.is-hidden-frameless:not(.is-fullscreen) .workspace-tabs.mod-top-right-space .workspace-tab-header-container:after
{
	position:absolute;
	top:0px;
	right:0px;
	width:var(--titleBarWidth);
	height:var(--tabHeight);
	min-height:20px;
}
.disableWindowMargins .is-hidden-frameless:not(.is-fullscreen) .workspace-tabs.mod-top-right-space .workspace-tab-header-container:after
{
height:var(--dragSize);
}
.is-hidden-frameless:not(.is-fullscreen) .mod-right-split .workspace-tabs.mod-top-right-space .workspace-tab-header-container
{
	margin-right: var(--titlebarOffset);
}
.stackTabs .mod-top-right-space .view-header
{
	margin-right:var(--titlebarOffset);
}
