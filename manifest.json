	*/
}
/* @settings
name: Micro Mike
id: a-unique-id
settings:
	-
		id: my-title
		title: My Settings
		type: heading
		level: 3
	-
		id: ribbonWidth
		title: Ribbon Button Width
		description: Height of buttons on side ribbon.
		type: variable-number-slider
		default: 40
		min: 30
		max: 90
		step: 1
		format: px
	-
		id: ribbonButtonHeight
		title: Ribbon Button Height
		description: Width of buttons on side ribbon.
		type: variable-number-slider
		default: 40
		min: 10
		max: 90
		step: 1
		format: px
	-
		id: dragHeight
		title: Drag Height
		description: Size of draggable region on top of window.
		type: variable-number-slider
		default: 10
		min: 0
		max: 40
		step: 1
		format: px
	-
		id: handleSize
		title: Side Handle Thickness
		description: Resize handle thickness. 
		type: variable-number-slider
		default: 8
		min: 4
		max: 32
		step: 2
		format: px
	-
		id: tabHeightSidebar
		title: Sidebar Tab Height
		description: Tab Height 
		type: variable-number-slider
		default: 40
		min: 20
		max: 40
		step: 1
		format: px
	-
		id: tabHeightEditor
		title: Editor Tab Height
		description: Tab Height 
		type: variable-number-slider
		default: 40
		min: 20
		max: 40
		step: 1
		format: px
	-
		id: fileHeight
		title: File Section Height
		description: The height of the file tabs.
		type: variable-number-slider
		default: 14
		min: 14
		max: 30
		step: 2
		format: px
	-
		id: outlineLevelWidth
		title: Outline Level Size
		description: The size of each level's separation. This affects the size of the collapse buttons.
		type: variable-number-slider
		default: 40
		min: 20
		max: 40
		step: 2
		format: px
	-
		id: outlineHeight
		title: Outline Section Height
		description: The height of the outline tabs. This affects the size of the collapse buttons.
		type: variable-number-slider
		default: 16
		min: 14
		max: 40
		step: 1
		format: px
	-
		id: editorSize
		title: Text Window Length
		description: Size of text window in text editor. 
		type: variable-number-slider
		default: 700
		min: 300
		max: 1800
		step: 50
		format: px
	-
		id: resizeBarsSize
		title: Resize Bars Size
		description: The height of the outline tabs. This affects the size of the collapse buttons.
		type: variable-number-slider
		default: 16
		min: 4
		max: 30
		step: 2
		format: px
	-
		id: stackTabs
		title: Stack the tabs
		description: The tabs will be stacked.
		type: class-toggle
		default: true
	-
		id: editorStackWidth
		title: Editor Stack Width
		description: The height of the outline tabs. This affects the size of the collapse buttons.
		type: variable-number-slider
		default: 200
		min: 120
		max: 300
		step: 5
		format: px
	-
		id: tabHeightEditorStack
		title: Editor Stack Tab Height
		description: Tab Height 
		type: variable-number-slider
		default: 40
		min: 20
		max: 40
		step: 1
		format: px
	-
		id: folderNoteEmphasis
		title: Folder Note Emphasis 
		description: The tabs will be stacked.
		type: class-toggle
	-
		id: disableBevels
		title: Disable Bevels 
		description: The tabs will be stacked.
		type: class-toggle
*/*, *:after, *:before {
  /*CSS transitions*/
  -webkit-transition-property: none !important;
  transition-property: none !important;

  -webkit-transition: none !important;
  transition: none !important;
  
  /*CSS transforms*/
  /* -webkit-transform: none !important; */
  /* transform: none !important; */

  /*CSS animations*/
  -webkit-animation: none !important;
  animation: none !important;
}
