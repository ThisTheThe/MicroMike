  --background-primary: var(--level1);
  --background-primary-alt: var(--level1_a);
  --background-secondary: var(--level1);
}body
{
	/* Hierarchical palette */
	--bottom: #000000;
	--bottom_a:#111111;
	--bottom_b:#222222;
	--level1: #333333;
	--level1_a: #444444;
	--level1_b: #4b4b4b;
	--level2: #4b4b4b;
	--level2_a: #555555;
	--level2_b: #5b5b5b;
	--level3: #555555;
	--level3_a: #666666;
	--collapseIconColor: #999999;

	--text-normal: #a3a5aa;
	/* Bevels */
	
	--bevelBright1: #a3a5aa;
	--bevelBright2: #8B8D91;
	--bevelDark1: #5A5A5A; 
	--bevelDark2: #7A7672; 
	/*
	--bevelBright1: red;
	--bevelBright2: green;
	--bevelDark1: yellow; 
	--bevelDark2: purple; 
	*/

}
body:not(.disableBevels) {
	--bevelStyleButton: inset -1px -1px var(--bevelDark1), inset 1px 1px var(--bevelBright2), inset -2px -2px var(--bevelDark2), inset 2px 2px var(--bevelBright1);
	--bevelStyleContainer: inset -1px -1px var(--bevelBright1), inset 1px 1px var(--bevelDark2), inset -2px -2px var(--bevelBright2), inset 2px 2px var(--bevelDark1);
    --bevelStyleContainerOuter:
    1px 1px var(--bevelBright1),
    -1px -1px var(--bevelDark2),
    -2px -2px var(--bevelDark1),
    2px 2px var(--bevelBright2),
    -2px 0px var(--bevelDark1),
    0px -2px var(--bevelDark1),
    -2px 2px var(--bevelBright2),
    2px -2px var(--bevelBright2);
    --bevelStyleTabActive: inset -1px 0 var(--bevelDark1), inset 1px 1px var(--bevelBright2), inset -2px 0 var(--bevelDark2), inset 2px 2px var(--bevelBright1);
    --bevelStyleFile: inset 1px 1px var(--level1);
    --bevelStyleFileHover: inset 1px 1px var(--level1_a);
    --bevelStyleFolder: inset 1px 1px var(--level2);
    --bevelStyleFolderHover:inset 1px 1px var(--level2_a);
    --bevelStyleFolderLabel:inset 0px 1px var(--level2_b);
    --bevelStyleFolderLabelHover:inset 0px 1px var(--level3_a);
    --bevelStyleTreeItem: inset 1px 1px var(--level2);
}.theme-dark
{
	--input-shadow: var(--bevelStyleButton);
	--input-shadow-hover: var(--bevelStyleButton);
	--interactive-normal: var(--level2);
	--interactive-hover: var(--level2_a);
}
.clickable-icon,
.clickable-icon:hover,
.workspace-tab-header-inner-close-button,
button
{
	box-shadow:var(--bevelStyleButton);
